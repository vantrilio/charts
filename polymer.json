{
    "entrypoint": "index.html",
    "shell": "src/gnc-app.html",
    "fragments": [
        "src/gnc-privacy-statement.html",
        "src/gnc-scenario.html",
        "src/gnc-terms-and-conditions.html",
        "src/gnc-view404.html",
        "src/gnc-welcome.html"
